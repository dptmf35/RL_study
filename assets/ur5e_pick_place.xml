<mujoco model="ur5e_pick_place">
  <compiler angle="radian" meshdir="ur5e/assets" autolimits="true"/>

  <option integrator="implicitfast" timestep="0.002"/>

  <default>
    <default class="ur5e">
      <material specular="0.5" shininess="0.25"/>
      <joint axis="0 1 0" range="-6.28319 6.28319" armature="0.1"/>
      <general gaintype="fixed" biastype="affine" ctrlrange="-6.2831 6.2831" gainprm="2000" biasprm="0 -2000 -400"
        forcerange="-150 150"/>
      <default class="size3">
        <default class="size3_limited">
          <joint range="-3.1415 3.1415"/>
          <general ctrlrange="-3.1415 3.1415"/>
        </default>
      </default>
      <default class="size1">
        <general gainprm="500" biasprm="0 -500 -100" forcerange="-28 28"/>
      </default>
      <default class="visual">
        <geom type="mesh" contype="0" conaffinity="0" group="2"/>
      </default>
      <default class="collision">
        <geom type="capsule" group="3"/>
        <default class="eef_collision">
          <geom type="cylinder"/>
        </default>
      </default>
      <site size="0.001" rgba="0.5 0.5 0.5 0.3" group="4"/>
    </default>
    <default class="gripper">
      <joint type="slide" axis="1 0 0" limited="true" range="0 0.04" damping="100" armature="0.01"/>
      <geom type="box" size="0.01 0.02 0.04" rgba="0.5 0.5 0.5 1" friction="1 0.05 0.01"/>
    </default>
  </default>

  <asset>
    <material class="ur5e" name="black" rgba="0.033 0.033 0.033 1"/>
    <material class="ur5e" name="jointgray" rgba="0.278 0.278 0.278 1"/>
    <material class="ur5e" name="linkgray" rgba="0.82 0.82 0.82 1"/>
    <material class="ur5e" name="urblue" rgba="0.49 0.678 0.8 1"/>
    <material name="table_mat" rgba="0.8 0.6 0.4 1" specular="0.1" shininess="0.1"/>
    <material name="cube_mat" rgba="0.8 0.2 0.2 1" specular="0.5" shininess="0.3"/>
    <material name="target_mat" rgba="0.2 0.8 0.2 0.5" specular="0.1" shininess="0.1"/>
    <material name="gripper_mat" rgba="0.3 0.3 0.3 1" specular="0.5" shininess="0.3"/>

    <mesh file="base_0.obj"/>
    <mesh file="base_1.obj"/>
    <mesh file="shoulder_0.obj"/>
    <mesh file="shoulder_1.obj"/>
    <mesh file="shoulder_2.obj"/>
    <mesh file="upperarm_0.obj"/>
    <mesh file="upperarm_1.obj"/>
    <mesh file="upperarm_2.obj"/>
    <mesh file="upperarm_3.obj"/>
    <mesh file="forearm_0.obj"/>
    <mesh file="forearm_1.obj"/>
    <mesh file="forearm_2.obj"/>
    <mesh file="forearm_3.obj"/>
    <mesh file="wrist1_0.obj"/>
    <mesh file="wrist1_1.obj"/>
    <mesh file="wrist1_2.obj"/>
    <mesh file="wrist2_0.obj"/>
    <mesh file="wrist2_1.obj"/>
    <mesh file="wrist2_2.obj"/>
    <mesh file="wrist3.obj"/>

    <texture type="2d" name="grid_texture" builtin="checker" width="512" height="512" rgb1="0.9 0.9 0.9" rgb2="0.7 0.7 0.7"/>
    <material name="floor_mat" texture="grid_texture" texrepeat="10 10" specular="0.1" shininess="0.1"/>
  </asset>

  <worldbody>
    <!-- Lighting -->
    <light name="top_light" pos="0 0 2" dir="0 0 -1" diffuse="0.8 0.8 0.8" specular="0.3 0.3 0.3"/>
    <light name="spotlight" mode="targetbodycom" target="wrist_2_link" pos="0 -1 2"/>

    <!-- Floor -->
    <geom name="floor" type="plane" size="2 2 0.1" pos="0 0 0" material="floor_mat"/>

    <!-- Table - positioned in front of robot's reachable workspace -->
    <body name="table" pos="0 0.45 0">
      <geom name="table_top" type="box" size="0.4 0.3 0.02" pos="0 0 0.4" material="table_mat"/>
      <geom name="table_leg1" type="cylinder" size="0.03 0.2" pos="0.35 0.25 0.2" material="table_mat"/>
      <geom name="table_leg2" type="cylinder" size="0.03 0.2" pos="0.35 -0.25 0.2" material="table_mat"/>
      <geom name="table_leg3" type="cylinder" size="0.03 0.2" pos="-0.35 0.25 0.2" material="table_mat"/>
      <geom name="table_leg4" type="cylinder" size="0.03 0.2" pos="-0.35 -0.25 0.2" material="table_mat"/>
    </body>

    <!-- Target area (visual only) - in robot's reachable area -->
    <body name="target" pos="0.12 0.42 0.42">
      <geom name="target_area" type="cylinder" size="0.05 0.001" material="target_mat" contype="0" conaffinity="0"/>
      <site name="target_site" pos="0 0 0.025" size="0.01" rgba="0 1 0 1"/>
    </body>

    <!-- Cube object - spawns in robot's reachable workspace -->
    <body name="cube" pos="-0.05 0.42 0.45">
      <freejoint name="cube_joint"/>
      <geom name="cube_geom" type="box" size="0.025 0.025 0.025" mass="0.1" material="cube_mat"
            friction="1 0.05 0.01" solref="0.02 1"/>
      <site name="cube_site" pos="0 0 0" size="0.01" rgba="1 0 0 1"/>
    </body>

    <!-- UR5e Robot -->
    <body name="base" pos="0 0 0.42" quat="0 0 0 -1" childclass="ur5e">
      <inertial mass="4.0" pos="0 0 0" diaginertia="0.00443333156 0.00443333156 0.0072"/>
      <geom mesh="base_0" material="black" class="visual"/>
      <geom mesh="base_1" material="jointgray" class="visual"/>
      <geom type="cylinder" size="0.06 0.05" pos="0 0 0" class="collision"/>

      <body name="shoulder_link" pos="0 0 0.163">
        <inertial mass="3.7" pos="0 0 0" diaginertia="0.0102675 0.0102675 0.00666"/>
        <joint name="shoulder_pan_joint" class="size3" axis="0 0 1"/>
        <geom mesh="shoulder_0" material="urblue" class="visual"/>
        <geom mesh="shoulder_1" material="black" class="visual"/>
        <geom mesh="shoulder_2" material="jointgray" class="visual"/>
        <geom class="collision" size="0.06 0.06" pos="0 0 -0.04"/>

        <body name="upper_arm_link" pos="0 0.138 0" quat="1 0 1 0">
          <inertial mass="8.393" pos="0 0 0.2125" diaginertia="0.133886 0.133886 0.0151074"/>
          <joint name="shoulder_lift_joint" class="size3"/>
          <geom mesh="upperarm_0" material="linkgray" class="visual"/>
          <geom mesh="upperarm_1" material="black" class="visual"/>
          <geom mesh="upperarm_2" material="jointgray" class="visual"/>
          <geom mesh="upperarm_3" material="urblue" class="visual"/>
          <geom class="collision" pos="0 -0.04 0" quat="1 1 0 0" size="0.06 0.06"/>
          <geom class="collision" size="0.05 0.2" pos="0 0 0.2"/>

          <body name="forearm_link" pos="0 -0.131 0.425">
            <inertial mass="2.275" pos="0 0 0.196" diaginertia="0.0311796 0.0311796 0.004095"/>
            <joint name="elbow_joint" class="size3_limited"/>
            <geom mesh="forearm_0" material="urblue" class="visual"/>
            <geom mesh="forearm_1" material="linkgray" class="visual"/>
            <geom mesh="forearm_2" material="black" class="visual"/>
            <geom mesh="forearm_3" material="jointgray" class="visual"/>
            <geom class="collision" pos="0 0.08 0" quat="1 1 0 0" size="0.055 0.06"/>
            <geom class="collision" size="0.038 0.19" pos="0 0 0.2"/>

            <body name="wrist_1_link" pos="0 0 0.392" quat="1 0 1 0">
              <inertial mass="1.219" pos="0 0.127 0" diaginertia="0.0025599 0.0025599 0.0021942"/>
              <joint name="wrist_1_joint" class="size1"/>
              <geom mesh="wrist1_0" material="black" class="visual"/>
              <geom mesh="wrist1_1" material="urblue" class="visual"/>
              <geom mesh="wrist1_2" material="jointgray" class="visual"/>
              <geom class="collision" pos="0 0.05 0" quat="1 1 0 0" size="0.04 0.07"/>

              <body name="wrist_2_link" pos="0 0.127 0">
                <inertial mass="1.219" pos="0 0 0.1" diaginertia="0.0025599 0.0025599 0.0021942"/>
                <joint name="wrist_2_joint" axis="0 0 1" class="size1"/>
                <geom mesh="wrist2_0" material="black" class="visual"/>
                <geom mesh="wrist2_1" material="urblue" class="visual"/>
                <geom mesh="wrist2_2" material="jointgray" class="visual"/>
                <geom class="collision" size="0.04 0.06" pos="0 0 0.04"/>
                <geom class="collision" pos="0 0.02 0.1" quat="1 1 0 0" size="0.04 0.04"/>

                <body name="wrist_3_link" pos="0 0 0.1">
                  <inertial mass="0.1889" pos="0 0.0771683 0" quat="1 0 0 1"
                    diaginertia="0.000132134 9.90863e-05 9.90863e-05"/>
                  <joint name="wrist_3_joint" class="size1"/>
                  <geom material="linkgray" mesh="wrist3" class="visual"/>
                  <geom class="eef_collision" pos="0 0.08 0" quat="1 1 0 0" size="0.04 0.02"/>

                  <!-- End effector site -->
                  <site name="ee_site" pos="0 0.1 0" quat="-1 1 0 0" size="0.01" rgba="0 0 1 1"/>

                  <!-- Simple parallel gripper -->
                  <body name="gripper_base" pos="0 0.1 0" quat="-1 1 0 0">
                    <inertial mass="0.5" pos="0 0 0.05" diaginertia="0.001 0.001 0.001"/>
                    <geom type="box" size="0.04 0.02 0.03" pos="0 0 0.03" material="gripper_mat"/>

                    <!-- Left finger -->
                    <body name="left_finger" pos="-0.04 0 0.06">
                      <inertial mass="0.1" pos="0 0 0.03" diaginertia="0.0001 0.0001 0.0001"/>
                      <joint name="finger_left_joint" class="gripper"/>
                      <geom name="left_finger_geom" type="box" size="0.01 0.015 0.04" pos="0 0 0.04"
                            material="gripper_mat" friction="1 0.05 0.01"/>
                      <site name="left_finger_site" pos="0 0 0.08" size="0.005" rgba="1 1 0 1"/>
                    </body>

                    <!-- Right finger -->
                    <body name="right_finger" pos="0.04 0 0.06">
                      <inertial mass="0.1" pos="0 0 0.03" diaginertia="0.0001 0.0001 0.0001"/>
                      <joint name="finger_right_joint" class="gripper" axis="-1 0 0"/>
                      <geom name="right_finger_geom" type="box" size="0.01 0.015 0.04" pos="0 0 0.04"
                            material="gripper_mat" friction="1 0.05 0.01"/>
                      <site name="right_finger_site" pos="0 0 0.08" size="0.005" rgba="1 1 0 1"/>
                    </body>

                    <!-- Gripper center site for grasping -->
                    <site name="gripper_center" pos="0 0 0.1" size="0.01" rgba="0 1 1 1"/>
                  </body>
                </body>
              </body>
            </body>
          </body>
        </body>
      </body>
    </body>
  </worldbody>

  <actuator>
    <!-- Arm actuators -->
    <general class="size3" name="shoulder_pan" joint="shoulder_pan_joint"/>
    <general class="size3" name="shoulder_lift" joint="shoulder_lift_joint"/>
    <general class="size3_limited" name="elbow" joint="elbow_joint"/>
    <general class="size1" name="wrist_1" joint="wrist_1_joint"/>
    <general class="size1" name="wrist_2" joint="wrist_2_joint"/>
    <general class="size1" name="wrist_3" joint="wrist_3_joint"/>
    <!-- Gripper actuators -->
    <position name="gripper_left" joint="finger_left_joint" kp="1000" ctrlrange="0 0.04"/>
    <position name="gripper_right" joint="finger_right_joint" kp="1000" ctrlrange="0 0.04"/>
  </actuator>

  <sensor>
    <!-- Joint position sensors -->
    <jointpos name="shoulder_pan_pos" joint="shoulder_pan_joint"/>
    <jointpos name="shoulder_lift_pos" joint="shoulder_lift_joint"/>
    <jointpos name="elbow_pos" joint="elbow_joint"/>
    <jointpos name="wrist_1_pos" joint="wrist_1_joint"/>
    <jointpos name="wrist_2_pos" joint="wrist_2_joint"/>
    <jointpos name="wrist_3_pos" joint="wrist_3_joint"/>
    <!-- Joint velocity sensors -->
    <jointvel name="shoulder_pan_vel" joint="shoulder_pan_joint"/>
    <jointvel name="shoulder_lift_vel" joint="shoulder_lift_joint"/>
    <jointvel name="elbow_vel" joint="elbow_joint"/>
    <jointvel name="wrist_1_vel" joint="wrist_1_joint"/>
    <jointvel name="wrist_2_vel" joint="wrist_2_joint"/>
    <jointvel name="wrist_3_vel" joint="wrist_3_joint"/>
    <!-- Gripper sensors -->
    <jointpos name="gripper_left_pos" joint="finger_left_joint"/>
    <jointpos name="gripper_right_pos" joint="finger_right_joint"/>
    <!-- Touch sensors -->
    <touch name="left_finger_touch" site="left_finger_site"/>
    <touch name="right_finger_touch" site="right_finger_site"/>
  </sensor>

  <keyframe>
    <key name="home" qpos="-1.5708 -1.5708 1.5708 -1.5708 -1.5708 0 0 0 0.4 -0.2 0.475 1 0 0 0"
         ctrl="-1.5708 -1.5708 1.5708 -1.5708 -1.5708 0 0 0"/>
  </keyframe>
</mujoco>
